funcao soma(a, b) {
    retorne a + b;
}

funcao subtrair(a, b) {
    retorne a - b;
}

variavel x = soma(5, 10);
variavel y = subtrair(x, 2);

escreva("x = ");
escreva(x);
escreva("\n");

escreva("y = ");
escreva(y);
escreva("\n");

se (y > 10) {
    escreva("y é maior que 10\n");
} senao {
    escreva("y não é maior que 10\n");
}
