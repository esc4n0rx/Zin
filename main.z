funcao soma(a, b) {
    retorne a + b;
}

variavel resultado = soma(7, 3);
escreva("O resultado da soma é: " + resultado + "\n");

funcao digaOi() {
    escreva("Oi dentro da função!\n");
    retorne 123;
}

variavel testeRetorno = digaOi();
escreva("A função digaOi() retornou: " + testeRetorno + "\n");
